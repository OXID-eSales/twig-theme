{% for oNewPaymentMethod in oView.getNotMappedToRDFaPayments() %}
    <div about="{{ sRDFaUrl|raw }}#{{ oNewPaymentMethod.oxpayments__oxdesc.value|regex_replace("/\s+/","")|cat('_')|cat(oNewPaymentMethod.oxpayments__oxid.value) }}" typeof="gr:PaymentMethod">
        <div property="rdfs:label" content="{{ oNewPaymentMethod.oxpayments__oxdesc.value }}"></div>
        {% if oNewPaymentMethod.oxpayments__oxlongdesc.value != "" %}
            <div property="rdfs:comment" content="{{ oNewPaymentMethod.oxpayments__oxlongdesc.value|striptags|regex_replace("/\s+/"," ") }}"></div>
        {% endif %}
    </div>
{% endfor %}
